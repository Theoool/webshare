    "class-validator": "^0.14.1",
    "crypto-js": "^4.2.0",
    "generic-pool": "^3.9.0",
    "htmlparser2": "^10.0.0",
    "ioredis": "^5.6.0",
    "langchain": "^0.3.19",
    "multer": "1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "node-summarizer": "^1.0.7",
    "openai": "^4.85.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pdf-parse": "^1.1.1",
    "puppeteer": "^24.2.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "typesense": "^1.8.2",
    "uuid": "^11.1.0"
